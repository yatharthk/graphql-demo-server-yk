type Mutation{
    createStudent(createStudentRequest: StudentRequestInput):StudentResponse
}

input StudentRequestInput{
    firstName: String,
    lastName: String,
    email:String,
    street :String,
    city:String,
    subjectsLearning: [CreateSubjectRequest]
}

input CreateSubjectRequest{
    subjectName:String,
    marksObtained:Float
}